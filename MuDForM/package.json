{
  "name": "@freon4dsl/webapp-starter",
  "version": "1.1.0-beta.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "initialize": "npm run clean && npm install && npm run build",
    "clean": "rm -rf node_modules/ public/build/dist/ package.lock.json",
    "build": "npm run generate && tsc",
    "build-app": "vite build",
    "build-dev": "cd ../../packages/samples/CourseSchedule && npm run build",
    "dev": "vite",
    "generate": "freon all -d src/defs -o src/freon",
    "preview": "vite preview",
    "start": "sirv public --port 8000",
    "validate": "svelte-check --tsconfig ./tsconfig.json",
    "errors-only": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "install-local": "npm install --registry http://localhost:4873",
    "prepare-app": "npm run styles-light && npm run styles-dark",
    "styles-light": "smui-theme compile public/site.css -i style",
    "styles-dark": "smui-theme compile public/site-dark.css -i style/dark",
    "format": "prettier --write .",
    "server": "node ./node_modules/@freon4dsl/server/dist/server/server-starter.js"
  },
  "devDependencies": {
    "@freon4dsl/meta": "1.1.0-beta.3",
    "@freon4dsl/server": "1.1.0-beta.3",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@smui-extra/accordion": "^8.0.0-beta.3",
    "@tsconfig/svelte": "^5.0.4",
    "rollup": "^4.22.4",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.2.0",
    "@rollup/plugin-terser": "^0.4.4",
    "smui-theme": "^8.0.0-beta.3",
    "svelte-check": "^4.1.5",
    "svelte-preprocess": "^6.0.3",
    "typescript": "5.4.5",
    "vite": "^6.0.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3"
  },
  "dependencies": {
    "@freon4dsl/core": "1.1.0-beta.3",
    "@freon4dsl/core-svelte": "1.1.0-beta.3",
    "@freon4dsl/webapp-lib": "1.1.0-beta.3",
    "@smui/snackbar": "^8.0.0-beta.3",
    "@smui-extra/accordion": "^8.0.0-beta.3",
    "@smui/data-table": "^8.0.0-beta.3",
    "@types/node": "^20.14.1",
    "kotlin": "^1.5.21",
    "lodash": "^4.17.21",
    "mobx": "^6.12.3",
    "mobx-utils": "^6.0.8",
    "net.akehurst.language-agl-processor": "4.2.1-20-RC2",
    "sirv-cli": "^2.0.2",
    "tslib": "^2.6.2",
    "@smui/radio": "^8.0.0-beta.3",
    "@smui/card": "^8.0.0-beta.3",
    "@smui/linear-progress": "^8.0.0-beta.3",
    "@smui/tab": "^8.0.0-beta.3",
    "@smui/textfield": "^8.0.0-beta.3",
    "@smui/form-field": "^8.0.0-beta.3",
    "@smui/drawer": "^8.0.0-beta.3",
    "@smui/layout-grid": "^8.0.0-beta.3",
    "@smui/banner": "^8.0.0-beta.3",
    "@smui/top-app-bar": "^8.0.0-beta.3",
    "@smui/dialog": "^8.0.0-beta.3",
    "@smui/tab-bar": "^8.0.0-beta.3"
  }
}
